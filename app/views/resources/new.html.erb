<h1>New Resource</h1>

<%= form_for @resource do |f| %>
<%= f.label :title %>
<%= f.text_field :title %>
<%= f.label :description %>
<%= f.text_field :description %>
<%= f.label :image %>
<%= f.text_field :image %>
<%= f.label :resource_type %>
<%= f.select :resource_type, ["Video", "Book", "Phone Number", "Website", "Shelter", "Place to Eat", "Place to Sleep" ] %>
<%= f.submit %>
<% end %>